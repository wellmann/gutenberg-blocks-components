{
  "name": "@wellmann/gutenberg-blocks-components",
  "version": "2.0.0",
  "description": "Accompanying npm package for https://github.com/wellmann/gutenberg-blocks-framework.",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "babel --config-file ./configs/.babelrc.js src/ --out-dir dist --extensions '.ts,.tsx,.js'",
    "type-check": "tsc --project ./configs/tsconfig.json --noEmit --emitDeclarationOnly false",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "author": {
    "name": "Kevin Wellmann",
    "url": "http://kevin.wellmann.io"
  },
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/wellmann/gutenberg-blocks-components.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.scss": [
      "suppress-exit-code stylelint --fix"
    ],
    "*.js": [
      "suppress-exit-code eslint --fix"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.14.6",
    "@types/wordpress__components": "^23.0.1",
    "@types/wordpress__wordcount": "^2.4.2",
    "@wordpress/eslint-plugin": "^12.9.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "stylelint": "^14.16.1",
    "suppress-exit-code": "^1.0.0"
  },
  "dependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
    "@svgr/webpack": "^6.5.1",
    "@wordpress/components": "^23.5.0",
    "@wordpress/compose": "^6.5.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^3.7.0",
    "@wordpress/element": "^5.5.0",
    "@wordpress/i18n": "^4.28.0",
    "@wordpress/wordcount": "^3.28.0",
    "babel-loader": "^8.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.3",
    "fast-glob": "^3.2.12",
    "highlight.js": "^11.7.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass-json-importer": "^4.3.0",
    "react-bem-helper": "^1.4.1",
    "react-simple-code-editor": "^0.13.1",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.10.0"
  },
  "browser": {
    "crypto": false
  }
}
