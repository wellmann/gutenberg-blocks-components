    "*.js": [
      "suppress-exit-code eslint --fix"
    ]
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-react": "^7.21.4",
    "husky": "^1.3.1",
